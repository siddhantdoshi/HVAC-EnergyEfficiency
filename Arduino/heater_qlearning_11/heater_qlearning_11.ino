  analogWrite(HEATER_PWM, 0); // Sets heater to off

  action_table_size = sizeof(q_actions)/sizeof(q_actions[0]);
  
  Serial.begin(9600);
  Serial.println("Timestamp, Temperature (ÂºC), Setpoint, Error, Delta_T, State, PWM output, Current, PWM / Current");
