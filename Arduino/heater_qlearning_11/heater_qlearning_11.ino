  Serial.print(temperature - set_point - 0.2);
  
  Serial.print(", ");
  Serial.print(velocity);
  
  Serial.print(", ");
  Serial.print(state);

  Serial.print(", ");
  Serial.print(pwm_output);
  
  Serial.print(", ");
  Serial.print(current);

  Serial.print(", ");
  Serial.println(pwm_output / current);

  prev_temp = temperature ;
  count ++;
}
